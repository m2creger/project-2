<% include ./partials/header.ejs %>

<h3>Project list</h3>
<div class="card-group">
  <% posts.forEach(function(post) { %>
    <div class="card" style="width: 20rem;">
    <img class="card-img-top" src="https://farm5.staticflickr.com/4346/36407297135_d5cdf117a1.jpg" alt="Card image cap">
    <div class="card-block">
      <h4 class="card-title"><%= post.newIdea %></h4>
      <h4></h4>
      <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
      <a href="/editproject/<%= post._id %>" class="btn btn-primary">Edit Project</a>
    </div>
  </div>
  <% }) %>
  
</div>
<% include ./partials/footer.ejs %>