<% include ./partials/header.ejs %>

<h3>Project list</h3>
<div class="card-group">
  <% posts.forEach(function(post) { %>
    <div class="card" style="width: 20rem;">
  
        <img class="card-img-top" src="<%= post.pictures[0] %>">
    
    <div class="card-block">
      <h4 class="card-title"><%= post.newIdea %></h4>
      <h4></h4>
      <ul class="list-group list-group-flush">
          <li class="list-group-item"><%= post.supplies %></li>
      </ul>
      <a href="/editproject/<%= post._id %>" class="btn btn-primary">Edit Project</a>
      <a href="/deleteproject/<%= post._id %>" class="btn btn-primary">Delete Project</a>
    </div>
  </div>
  <% }) %>
  
</div>
<% include ./partials/footer.ejs %>